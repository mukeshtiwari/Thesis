@InProceedings{10.1007/11814771_17,
author="Harrison, John",
editor="Furbach, Ulrich
and Shankar, Natarajan",
title="Towards Self-verification of {H}{O}{L} {L}ight",
booktitle="Automated Reasoning",
year="2006",
publisher="Springer Berlin Heidelberg",
address="Berlin, Heidelberg",
pages="177--191",
abstract="The HOL Light prover is based on a logical kernel consisting of about 400 lines of mostly functional OCaml, whose complete formal verification seems to be quite feasible. We would like to formally verify (i) that the abstract HOL logic is indeed correct, and (ii) that the OCaml code does correctly implement this logic. We have performed a full verification of an imperfect but quite detailed model of the basic HOL Light core, without definitional mechanisms, and this verification is entirely conducted with respect to a set-theoretic semantics within HOL Light itself. We will duly explain why the obvious logical and pragmatic difficulties do not vitiate this approach, even though it looks impossible or useless at first sight. Extension to include definitional mechanisms seems straightforward enough, and the results so far allay most of our practical worries.",
isbn="978-3-540-37188-5"
}

@Article{Appel2003,
author="Appel, Andrew W.
and Michael, Neophytos
and Stump, Aaron
and Virga, Roberto",
title="A Trustworthy Proof Checker",
journal="Journal of Automated Reasoning",
year="2003",
month="Nov",
day="01",
volume="31",
number="3",
pages="231--260",
abstract="Proof-carrying code (PCC) and other applications in computer security require machine-checkable proofs of properties of machine-language programs. The main advantage of the PCC approach is that the amount of code that must be explicitly trusted is very small: it consists of the logic in which predicates and proofs are expressed, the safety predicate, and the proof checker. We have built a minimal proof checker, and we explain its design principles and the representation issues of the logic, safety predicate, and safety proofs. We show that the trusted computing base (TCB) in such a system can indeed be very small. In our current system the TCB is less than 2,700 lines of code (an order of magnitude smaller even than other PCC systems), which adds to our confidence of its correctness.",
issn="1573-0670",
doi="10.1023/B:JARS.0000021013.61329.58",
url="https://doi.org/10.1023/B:JARS.0000021013.61329.58"
}

